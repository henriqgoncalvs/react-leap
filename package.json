{
  "name": "react-leap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.6.6",
    "@craco/craco": "^6.2.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@nivo/bar": "^0.73.1",
    "@nivo/core": "^0.73.0",
    "@nivo/pie": "^0.73.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "chakra-ui-autocomplete": "^1.4.3",
    "dayjs": "^1.10.6",
    "formik": "^2.2.9",
    "framer-motion": "^4",
    "history": "^5.0.1",
    "immer": "^9.0.6",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "parallax-js": "^3.1.0",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-helmet-async": "^1.0.9",
    "react-icons": "^4.2.0",
    "react-images-uploading": "^3.1.3",
    "react-query": "^3.21.0",
    "react-query-auth": "^1.1.0",
    "react-router": "^6.0.0-beta.2",
    "react-router-dom": "^6.0.0-beta.2",
    "react-scripts": "4.0.3",
    "react-scrollbars-custom": "^4.0.27",
    "react-spinners": "^0.11.0",
    "react-table": "^7.7.0",
    "styled-mixins": "^0.1.10",
    "tsargs": "^1.4.1",
    "type-fest": "^2.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9",
    "zustand": "^3.5.10"
  },
  "scripts": {
    "start": "craco start",
    "start:cli": "cross-env BROWSER=none craco start",
    "build": "craco build",
    "test": "is-ci-cli \"test:coverage\" \"test:jest\"",
    "test:jest": "craco test",
    "test:coverage": "CI=1 yarn test:jest -- --coverage",
    "eject": "react-scripts eject",
    "husky:postinstall": "husky install",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test start:cli http://localhost:3000 cy:open",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "start-server-and-test serve http://localhost:3000 cy:run",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "validate-and-build": "npm-run-all --parallel check-types check-format lint build",
    "validate": "npm-run-all --parallel check-types && lint-staged",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "commit": "commit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt-cli": "^13.1.0",
    "@mswjs/data": "^0.5.1",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "@types/palx": "^1.0.1",
    "@types/ramda": "^0.27.44",
    "@types/react-custom-scrollbars": "^4.0.8",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "commitlint": "^13.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.11.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "is-ci-cli": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.1.2",
    "msw": "^0.35.0",
    "nanoid": "^3.1.25",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn lint"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/test/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
